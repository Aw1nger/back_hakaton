    "components": {
        "schemas": {
            "AdminAddRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "minLength": 1
                    },
                    "role": {
                        "$ref": "#/components/schemas/RoleEnum"
                    }
                },
                "required": [
                    "email",
                    "role"
                ]
            },
            "PatchedAdminBanRequest": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    }
                }
            },
            "PatchedAdminUnbanRequest": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    }
                }
            },
            "RoleEnum": {
                "enum": [
                    "user",
                    "org-admin",
                    "dev",
                    "admin",
                    "root-admin"
                ],
                "type": "string",
                "description": "* `user` - Пользователь\n* `org-admin` - Администратор организации\n* `dev` - Разработчик\n* `admin` - Администратор платформы\n* `root-admin` - ГА платформы"
            },
            "Test": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "maxLength": 50,
                        "pattern": "^[-a-zA-Z0-9_]+$"
                    }
                },
                "required": [
                    "id",
                    "name"
                ]
            },
            "TestAddAnswerRequest": {
                "type": "object",
                "properties": {
                    "text": {
                        "type": "string",
                        "minLength": 1
                    },
                    "answer_number": {
                        "type": "integer"
                    },
                    "question_id": {
                        "type": "integer"
                    },
                    "speciality_id": {
                        "type": "integer"
                    }
                },
                "required": [
                    "answer_number",
                    "question_id",
                    "speciality_id",
                    "text"
                ]
            },
            "TestAddQuestionRequest": {
                "type": "object",
                "properties": {
                    "text": {
                        "type": "string",
                        "minLength": 1
                    },
                    "test_id": {
                        "type": "integer"
                    }
                },
                "required": [
                    "test_id",
                    "text"
                ]
            },
            "TestAnswerRequest": {
                "type": "object",
                "properties": {
                    "test_id": {
                        "type": "integer"
                    },
                    "answer_id": {
                        "type": "integer"
                    }
                },
                "required": [
                    "answer_id",
                    "test_id"
                ]
            },
            "TestCreateRequest": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "minLength": 1
                    }
                },
                "required": [
                    "name"
                ]
            },
            "User": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 40
                    }
                },
                "required": [
                    "email",
                    "password"
                ]
            },
            "UserAddRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "minLength": 1
                    },
                    "password": {
                        "type": "string",
                        "minLength": 1
                    },
                    "name": {
                        "type": "string",
                        "minLength": 1
                    }
                },
                "required": [
                    "email",
                    "password"
                ]
            },
            "UserConfirmEmailRequest": {
                "type": "object",
                "properties": {
                    "confirm-token": {
                        "type": "string",
                        "format": "uuid"
                    }
                },
                "required": [
                    "confirm-token"
                ]
            },
            "UserLoginRequest": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "minLength": 1
                    },
                    "password": {
                        "type": "string",
                        "minLength": 1
                    }
                },
                "required": [
                    "email",
                    "password"
                ]
            }
        }
    }
